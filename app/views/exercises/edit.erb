<h1>Exercise Entry [EDIT]</h1>
<form method="POST" action="/exercises">
  <input type="hidden" name="_method" value="PATCH">

  <label>Exercise Name</label>
  <input type="text" name="exercise[name]">
  <label>Sets</label>
  <input type="number" name="exercise[sets]" placeholder="#">
  <label>Reps</label>
  <input type="number" name="exercise[reps]" placeholder="#">
  <label>Weight(lbs)</label>
  <input type="number" name="exercise[weight_lbs]" placeholder="#">

  <input type="submit" id="submit" value="Submit">
</form>
